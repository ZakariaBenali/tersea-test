{
	"name": "tersea-test-technique",
	"version": "1.0.0",
	"description": "",
	"main": "build/server.js",
	"scripts": {
		"dev": "NODE_ENV=development nodemon ./src/server.ts",
		"start": "NODE_ENV=production node ./build/server.js",
		"build": "tsc",
		"lint": "eslint .",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"types:check": "tsc --noEmit",
		"typeorm": "ts-node ./node_modules/typeorm/cli",
		"migrations:create": "cross-var npm run typeorm migration:create ./src/migrations/%npm_config_name%",
		"migrations:generate": "cross-var  npm run typeorm -- -d ./src/data-source.ts migration:generate ./src/migrations/%npm_config_name%",
		"migrations:run": "npm run typeorm migration:run -- -d ./src/data-source.ts",
		"migrations:revert": "npm run typeorm migration:revert -- -d ./src/data-source.ts",
		"entity:create": "cross-var npm run typeorm entity:create ./src/entities/%npm_config_name%"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/node": "^18.15.11",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"cross-var": "^1.1.0",
		"eslint": "^8.37.0",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "5.0.3"
	},
	"dependencies": {
		"bcrypt": "^5.1.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.0",
		"nodemailer": "^6.9.1",
		"pg": "^8.4.0",
		"pg-protocol": "^1.6.0",
		"reflect-metadata": "^0.1.13",
		"typeorm": "0.3.12"
	}
}
